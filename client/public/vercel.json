{
  "redirects": [
    {
      "source": "https://elpdata.com/(.*)",
      "destination": "https://www.elpdata.com/$1",
      "permanent": true
    },
    {
      "source": "http://elpdata.com/(.*)",
      "destination": "https://www.elpdata.com/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}